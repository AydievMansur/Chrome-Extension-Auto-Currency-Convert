{
  "manifest_version": 2,

  "name": "AyMan Price",
  "description": "asd up1",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },

  "content_scripts": [
    {
      "matches": ["https://swap.gg/"],
      "js": ["script.js"]
    }
  ],

  "background": {
    "scripts": ["popup.js"]
  },

  "permissions": [
    "activeTab",
    "tabs",
    "bookmarks",
    "https://swap.gg/",
    "unlimitedStorage",
    "alarms"
  ]
}

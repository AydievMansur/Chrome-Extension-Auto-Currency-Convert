{
  "manifest_version": 2,
  "icons": { "128": "assets/img/AM.png" },
  "name": "AyMan Price",
  "description": "Convert currency",
  "version": "1.7",
  "browser_action": {
    "default_icon": "assets/img/AM.png"
  },
  "content_scripts": [
    {
     # "matches": ["https://swap.gg/*","https://amazon.com/*","https://dmarket.com/*","http "],
      "css": ["assets/css/style.css"],
      "js": ["assets/js/script.js"]
    }
  ],
  "permissions": ["storage","activeTab","declarativeContent"], #commented matches cuz has access to active Tab, user given prompt about read activeTab
  "web_accessible_resources": [ "js/*", "html/*", "css/*", "images/*" ],
  "content_security_policy": "script-src 'self'; object-src 'self'; 'unsafe-inline'"
}

{
  "manifest_version": 2,
  "name": "AyMan Price",
  "description": "Convert currency",
  "version": "1.0",
  "browser_action": {
    "default_icon": "AM.png",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://swap.gg/","https://dmarket.com/"],
      "js": ["jquery.js", "script.js"]
    }
  ],
  "permissions": ["storage","activeTab","declarativeContent"]
}
